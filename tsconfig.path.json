{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@theme": ["theme"],
      "@theme/*": ["theme/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@layout": ["layout"],
      "@layout/*": ["layout/*"],
      "@types": ["types"],
      "@types/*": ["types/*"],
      "@store": ["store"],
      "@store/*": ["store/*"]
    }
  }
}
