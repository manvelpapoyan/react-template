{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@theme": ["theme"],
      "@theme/*": ["theme/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@layout": ["layout"],
      "@layout/*": ["layout/*"],
      "@appTypes": ["appTypes"],
      "@appTypes/*": ["appTypes/*"],
      "@store": ["store"],
      "@store/*": ["store/*"],
      "@svgs": ["assets/svgs"],
      "@svgs/*": ["assets/svgs/*"]
    }
  }
}
